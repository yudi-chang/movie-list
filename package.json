    "prettier": "^3.3.3",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10",
    "vuetify": "^3.7.6"
